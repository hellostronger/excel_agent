# Google Cloud Configuration (可选，如果使用Google模型)
GOOGLE_CLOUD_PROJECT=your-gcp-project-id
GOOGLE_CLOUD_LOCATION=us-central1

# API Keys
GEMINI_API_KEY=your-gemini-api-key
SILICONFLOW_API_KEY=your-siliconflow-api-key

# Model Provider Selection: "google" | "siliconflow"
MODEL_PROVIDER=siliconflow

# SiliconFlow Models Configuration (硅基流动模型配置)
ROOT_AGENT_MODEL=qwen-plus
ORCHESTRATOR_MODEL=qwen-max  
WORKER_MODEL=qwen-turbo

# Alternative Google Models (备选Google模型)
# ROOT_AGENT_MODEL=gemini-2.5-flash
# ORCHESTRATOR_MODEL=gemini-2.5-pro
# WORKER_MODEL=gemini-2.5-flash

# SiliconFlow API Configuration
SILICONFLOW_BASE_URL=https://api.siliconflow.cn/v1
SILICONFLOW_TIMEOUT=300

# Agent Configuration
MAX_PARALLEL_AGENTS=4
FILE_ANALYSIS_DEPTH=comprehensive
ENABLE_CONCURRENT_ANALYSIS=true
MEMORY_THRESHOLD_MB=512

# Timeouts (in seconds)
FILE_PREPARATION_TIMEOUT=120
PARALLEL_ANALYSIS_TIMEOUT=300
RESPONSE_GENERATION_TIMEOUT=60

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=detailed

# Development Settings
DEBUG_MODE=false
ENABLE_METRICS=true